# LLM API (Groq is fast/cheap, OpenAI works too)
LLM_PROVIDER=groq
GROQ_API_KEY=your_groq_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Qdrant (Cloud)
QDRANT_URL=https://your-cluster-id.your-region.gcp.cloud.qdrant.io:6333
QDRANT_API_KEY=your_qdrant_api_key_here
